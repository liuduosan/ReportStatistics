package com.dc.flamingo.tools.entity;

import com.dc.flamingo.core.support.BaseEntity;

/**
 * 编号生成器
 * @Class Name NumbersGenerator
 * @Author lee
 * @Create In May 23, 2011
 */
public class IdGenerator extends BaseEntity{
	public static final String TABLE = "sys_tools_idGenerator";
	private static final long serialVersionUID = -1503693302132768086L;
	private String idKey;	//编号关键字
	private Long lastId;	//最后编号	
	private String descn;		//编号描述

	public String getIdKey() {
		return idKey;
	}
	public void setIdKey(String idKey) {
		this.idKey = idKey;
	}
	public Long getLastId() {
		return lastId;
	}
	public void setLastId(Long lastId) {
		this.lastId = lastId;
	}
	public String getDescn() {
		return descn;
	}
	public void setDescn(String descn) {
		this.descn = descn;
	}

}
